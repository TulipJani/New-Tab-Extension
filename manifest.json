{
  "manifest_version": 3,
  "name": "New Tab",
  "version": "1.0",
  "description": "A new tab",
  "permissions": ["topSites", "storage"],
  "host_permissions": [
    "https://www.google.com/",
    "https://*.googleapis.com/",
    "https://*/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}